package com.shivshankar.assignment3q1;



public class PostfixCalculation {
	public static int calculate(int op1,int op2,char ele)
	{
		switch(ele)
		{
		case '+':return op1+op2;
		case '-':return op1-op2;
		case '*':return op1*op2;
		case '/':return op1/op2;
		}
		return 0;
	}
	public static void main(String[] args) {
		String postfix="";
		for(int i=0;i<postfix.length();i++)
		{
			
		}
	}

}
