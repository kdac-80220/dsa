package com.shivshankar.assignment3q2;
class Stack{
	private int arr[];
	private int top;
	private final int SIZE;
	
	public Stack(int size)
	{
		SIZE=size;
		arr=new int[SIZE];
		top=-1;
	}
	
	public void push(int data)
	{
		top++;
		arr[top]=data;
	}
	
	public int pop()
	{
		top--;
		return arr[top+1];
	}
	
	public int peek()
	{
		return arr[top];
	}
	
	public int max
}
public class FindMaxFromStack {
	

	public static void main(String[] args) {
		
	}

}
