package com.shivshankar.assignment4q1;

public class List {
	
	public class Node{
		private int data;
		private Node next;
		
		public Node(int data)
		{
			this.data=data;
			next=null;
		}
	}
	
	public List(){
		private Node head;
		
	}
	
	public void addFirst(int value)
	{
		Node newnode=new Node(value);
		if(isEmpty())
			head=newnode;
	}
	
	public boolean isEmpty()
	{
		return head==null;
	}
	
}
