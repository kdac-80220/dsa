package com.shivshankar.listpractice;

public class List {
	public class Node{
		private int data;
		private Node next;
		
		public Node(int data) {
			this.data=data;
			next=null;
		}
	}
	
	private Node head;
	
	List(){
		head=null;
	}
	
	public boolean isEmpty()
	{
		return head==null;
	}
	
	public void addList(int data)
	{
		Node shambho=new Node(data);
		
		if(isEmpty())
		head=shambho;
	}
	
	public void displayList()
	{
		Node trav=head;
		if(isEmpty())
			System.out.println("List empty");
		while()
	}
}
