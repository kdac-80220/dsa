package com.sunbeam;

public class LinearQueue {
	
		int arr[];
		int rear,front;
		private final int SIZE;
	
		public LinearQueue(int size) {
			SIZE=size;
			arr=new int[SIZE];
			rear=-1;
			front=-1;
		}
		
		public void insert(int data)
		{
			rear++;
			arr[rear]=data;
		}
		
		public void delete()
		{
			front++;
		}
		
		public int peek() {
			
		}
	
}
