
public class BubbleSort {
	public static int BubbleSortMethod(int arr[])
	{
		int comparisons=0;
		for(int i=0;i<arr.length;i++)
		{
			for(int j=0;j<arr.length-1-i;j++)
			{
				if(arr[j]>arr[j+1])
				{
					comparisons
					int temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=arr[j];
				}
			}
		}
		return comparisons;
	}
	public static void main(String[] args) {
		
	}

}
